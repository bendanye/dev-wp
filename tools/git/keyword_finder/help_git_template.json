{
  "helps": [
    {
      "keyword": "git",
      "type": "command",
      "command": "ls ${GIT_PROJECT_DIR}"
    },
    {
      "keyword": "glog",
      "type": "command",
      "command": "git --git-dir=${GIT_PROJECT_DIR}/${1}/.git --work-tree=${GIT_PROJECT_DIR}/${1} log -n 20 --pretty=format:%s"
    }
  ]
}